{
    "type": "origins:active_self",
    "key": {
        "key": "key.origins.primary_active"
    },
    "cooldown": 100,
    "hud_render": {
        "should_render": true,
        "sprite_location": "chill:textures/item/bars/cleric_bars.png",
        "bar_index": 2
    },
    "entity_action": {
        "type": "origins:raycast",
        "distance": 128,
        "block": true,
        "entity": false,
        "shape_type": "visual",
        "fluid_handling": "none",
        "before_action": {
            "type": "origins:execute_command",
            "command": "tag @s add rouge_tp"
        },
        "block_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "tp @p[tag=rouge_tp] ~ ~1 ~"
                },
                {
                    "type": "origins:execute_command",
                    "command": "tag @p remove rouge_tp"
                }
            ]
        },
        "command_along_ray": "particle minecraft:crit",
        "command_step": 1,
        "command_along_ray_only_on_hit": true,
        "miss_action": {
            "type": "origins:change_resource",
            "resource": "chill:class/rogue/special/teleport/primary",
            "operation": "set",
            "change": 100
        }
    },
    "name": "Teleportation",
    "description": "Use your ability to teleport to a mob you are looking at"
}