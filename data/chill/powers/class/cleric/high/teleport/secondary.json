{
    "type": "origins:active_self",
    "key": {
        "key": "key.origins.secondary_active"
    },
    "cooldown": 100,
    "hud_render": {
        "should_render": true,
        "sprite_location": "chill:textures/item/bars/cleric_bars.png",
        "bar_index": 2
    },
    "entity_action": {
        "type": "origins:raycast",
        "distance": 128,
        "block": false,
        "shape_type": "visual",
        "fluid_handling": "none",
        "bientity_action": {
            "type": "origins:target_action",
            "action": {
                "type": "origins:execute_command",
                "command": "tag @s add druid_teleport"
            }
        },
        "before_action": {
            "type": "origins:execute_command",
            "command": "tag @e remove druid_teleport"
        },
        "hit_action": {
            "type": "origins:execute_command",
            "command": "tp @s @e[tag=druid_teleport, limit=1]"
        },
        "command_along_ray": "particle minecraft:crit",
        "command_step": 1,
        "command_along_ray_only_on_hit": true,
        "miss_action": {
            "type": "origins:change_resource",
            "resource": "chill:class/cleric/high/teleport/primary",
            "operation": "set",
            "change": 100
        }
    },
    "name": "Teleportation",
    "description": "Use your ability to teleport to a mob you're looking at"
}
