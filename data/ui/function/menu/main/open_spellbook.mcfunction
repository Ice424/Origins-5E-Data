#opens cleric druid fighter rouge tank and wizard spellbooks
execute if entity @p[tag=fighter] run function ui:menu/fighter/spellbook/open
execute if entity @p[tag=wizard] run function ui:menu/wizard/spellbook/open
execute if entity @p[tag=cleric] run function ui:menu/cleric/spellbook/open
execute if entity @p[tag=tank] run function ui:menu/tank/spellbook/open
execute if entity @p[tag=rogue] run function ui:menu/rogue/spellbook/open
execute if entity @p[tag=druid] run function ui:menu/druid/spellbook/open
